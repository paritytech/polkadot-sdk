Network: ../../environments/polkadot-kusama/bridge_hub_kusama_local_network.toml
Creds: config

# send wKSM back to Alice from Polkadot AH to Kusama AH
asset-hub-kusama-collator-1: run ../../environments/polkadot-kusama/helper.sh with "withdraw-reserve-assets-from-asset-hub-polkadot-local" within 120 seconds

